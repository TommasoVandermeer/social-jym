[env] 
time_limit = 50
time_step = 0.0125
robot_time_step = 0.25
val_size = 100 
test_size = 500 
randomize_attributes = false 


[reward] 
success_reward = 1 
collision_penalty = -0.25 
discomfort_dist = 0.2 
discomfort_penalty_factor = 0.5 


[sim] 
train_val_sim = circular_crossing_with_static_obstacles
test_sim = circular_crossing_with_static_obstacles
traffic_length = 14 
traffic_height = 3 
circle_radius = 7 
human_num = 6 


[humans] 
visible = true 
policy = orca
radius = 0.3
v_pref = 1.0
sensor = coordinates 


[robot] 
visible = false 
policy = none 
radius = 0.3
v_pref = 1.0
sensor = coordinates